{
  "name": "<%= projectName %>",
  "version": "<%= projectVersion %>",
  "description": "<%= projectDescription %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },<% if (projectType === 'Node CLI') { %>
  "bin": {
    "<%= projectName %>": "cli.js"
  },<% } %>
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo && ava --verbose"
  },
  "repository": "<%= githubUser %>/<%= projectName %>",
  "keywords": [ <% 
    let i=0; const length=projectTags.length;
    projectTags.forEach(function(tag){ %>
    "<%=tag.trim();%>"<% if(i !== length - 1){ %><%=','%><% i++;}}) %>
  ],
  "dependencies": {
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "xo": "^0.21.1"
  },
  "xo": {
    "space": 2
  },
  "license": "MIT"
}
